{
  "name": "icon-snarkjs",
  "type": "module",
  "version": "0.6.10",
  "description": "zkSNARKs implementation in JavaScript",
  "main": "./build/main.cjs",
  "module": "./main.js",
  "exports": {
    "import": "./main.js",
    "require": "./build/main.cjs"
  },
  "scripts": {
    "commit": "git-cz",
    "test": "mocha",
    "testv12": "mocha test/fflonk.test.js test/fullprocess.js test/keypar_test.js test/polynomial.test.js",
    "build": "rollup -c config/rollup.cjs.config.js",
    "buildcli": "rollup -c config/rollup.cli.config.js",
    "buildiife": "BROWSER=true rollup -c config/rollup.iife.config.js",
    "buildiifemin": "BROWSER=true rollup -c config/rollup.iife_min.config.js",
    "semantic-release": "semantic-release"
  },
  "bin": {
    "icon-snarkjs": "build/cli.cjs"
  },
  "directories": {
    "templates": "templates"
  },
  "keywords": [
    "zksnark",
    "zcash",
    "ethereum",
    "zero",
    "knowlage",
    "cryptography",
    "circuit",
    "icon"
  ],
  "author": "Jordi Baylina",
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/venture23-zkp/icon-snarkjs"
  },
  "dependencies": {
    "@iden3/binfileutils": "0.0.11",
    "bfj": "^7.0.2",
    "blake2b-wasm": "^2.4.0",
    "circom_runtime": "0.1.22",
    "ejs": "^3.1.6",
    "fastfile": "0.0.20",
    "ffjavascript": "0.2.57",
    "js-sha3": "^0.8.0",
    "logplease": "^1.2.15",
    "r1csfile": "0.0.45"
  },
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "^2.2.2",
    "@nomiclabs/hardhat-waffle": "^2.0.5",
    "@rollup/plugin-commonjs": "^22.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^14.0.0",
    "@rollup/plugin-replace": "^4.0.0",
    "@rollup/plugin-virtual": "^2.0.3",
    "chai": "^4.3.7",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.7.0",
    "ethereum-waffle": "^4.0.10",
    "ethers": "^5.7.2",
    "hardhat": "^2.13.0",
    "mocha": "^10.2.0",
    "rollup": "^2.36.2",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-visualizer": "^5.6.0",
    "semantic-release": "^20.1.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
